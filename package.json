{
  "name": "botbuilder",
  "version": "alpha",
  "description": "A discord bot setup that has anything a server owner could wish for.",
  "main": "build/index.js",
  "scripts": {
    "dev": "tsc-watch --onFirstSuccess \"node build/index.js\" --noClear --silent",
    "build": "tsc",
    "start": "node build/index.js"
  },
  "author": "SanukaE",
  "license": "MIT",
