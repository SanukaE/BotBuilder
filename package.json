    "#libs/*": "./build/libs/*"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "bing-translate-api": "^4.0.2",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "redis": "^4.7.0"
